<Application
    x:Class="gameslist.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:gameslist"
    xmlns:theme="using:gameslist.Theme"
    xmlns:vm="using:gameslist.ViewModel"
    xmlns:conver="using:gameslist.Conver"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <theme:NewBaseDictionary/>
            </ResourceDictionary.MergedDictionaries>
            <vm:VMLocator x:Key="vmlocator"/>
            <conver:IsNullOrEmptyConvertToVisibility x:Key="isNullOrEmptyConverVisibility"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
