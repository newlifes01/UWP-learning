<local:CommonPage
    x:Class="MyApp.Page2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MyApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <RelativePanel Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Rectangle Name="r1" Height="80" Fill="LightBlue" RelativePanel.AlignTopWithPanel="True" RelativePanel.AlignHorizontalCenterWithPanel="True" Width="300"/>
        <Rectangle Name="r2" RelativePanel.Below="r1" RelativePanel.AlignLeftWith="r1" Width="60" Height="350" Fill="#FFF0FDB7"/>
        <Rectangle RelativePanel.Below="r2" Width="300" Height="70" RelativePanel.AlignLeftWith="r2" Fill="#FFEAC0DB"/>
    </RelativePanel>
</local:CommonPage>
